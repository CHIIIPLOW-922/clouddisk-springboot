	<!--实体映射-->
	<resultMap id="adminResultMap" type="com.chiiiplow.clouddisk.entity.Admin">
		<!--admin id-->
		<id column="id" property="id"/>
		<!--admin account-->
		<result column="admin_account" property="adminAccount"/>
